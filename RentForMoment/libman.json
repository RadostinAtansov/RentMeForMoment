{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "aspnet-signalr@1.0.27",
      "destination": "wwwroot/lib/aspnet-signalr/"
    },
    {
      "provider": "unpkg",
      "library": "@aspnet/signalr@1.0.27",
      "destination": "wwwroot/js/signalr"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@5.0.9",
      "destination": "wwwroot/js/signalr",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    }
  ]
}